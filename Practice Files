# In[55]:


lower = 2
upper = 20


# In[58]:


print('prime numbers between',lower,'and', upper, 'are:')
for num in range (lower,upper + 2):
    if num > 1:
        for i in range (3,num):
            if (num % i) == 0:
                break
            else:
                print(num)

