# In[24]:


num = int(input('Enter a number:  '))
if (num % 2) == 0:
    print ('{0} is even'.format(num))
else:
    print('{0} is Odd'.format(num))


# In[38]:


year = 1966
if (year % 4) == 0:
    if (year % 100) == 0:
        if (year % 400) == 0:
            print ('{0} is a leap year'. format(year))
        else:
            print('{0} is not a leap year'.format(year))
    else:
        print('{0} is a leap year'. format(year))
else:
    print('{} is not a leap year'. format(year))


